{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*", "/css/*", "/api/*"]
  },
  "routes": [
    {
      "route": "/api/*",
      "methods": ["GET", "POST", "PUT", "DELETE"],
      "rewrite": "/api/index.js"
    }
  ],
  "platform": {
    "apiRuntime": "node:18"
  },
  "outputLocation": {
    "api": "api",
    "app": "dist"
  }
}