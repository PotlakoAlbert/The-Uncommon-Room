{
  "routes": [
    {
      "route": "/api/*",
      "serve": "/index.js",
      "methods": ["GET", "POST", "PUT", "DELETE"]
    },
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html"
  },
  "platform": {
    "apiRuntime": "node:18"
  },
  "mimeTypes": {
    ".js": "text/javascript",
    ".css": "text/css",
    ".html": "text/html"
  }
}