{
  "routes": [
    {
      "route": "/api/*",
      "rewrite": "/api/index.mjs"
    },
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/api/*", "/assets/*"]
  },
  "platform": {
    "apiRuntime": "node:18"
  },
  "functions": {
    "language": "node",
    "version": "18"
  }
}